-wip.effects.Gclj535PqwyJWZ3E]{vigor 3.}</p>","quantity":1,"weight":0,"attributes":{"Tags":{"Actions":{"value":"2 Actions","label":"1 action","dtype":"String","group":"Tags"},"Tag1":{"value":"Melee Attack","label":"Stance","dtype":"String","group":"Tags"}}},"groups":{"Tags":{"label":"Tags","dtype":"String"}}},"_stats":{"systemId":"worldbuilding","systemVersion":"0.7.2","coreVersion":"10.290","createdTime":1669629697425,"modifiedTime":1669629697425,"lastModifiedBy":"tXyJLVgERViy4mQ6"},"folder":null,"sort":0,"ownership":{"default":0,"tXyJLVgERViy4mQ6":3},"_id":"3MNzsFFn2ZIsncoi"}
