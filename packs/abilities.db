{"Tags":{"Actions":{"value":"2 Actions","label":"Heavy Ranged Attack","dtype":"String","group":"Tags"},"Tag1":{"value":"Melee Attack","label":"Line 5 + Cross 2","dtype":"String","group":"Tags"},"Tag2":{"value":"True Strike","label":"Combo","dtype":"String","group":"Tags"}}},"groups":{"Tags":{"label":"Tags","dtype":"String"}}},"_stats":{"systemId":"worldbuilding","systemVersion":"0.7.2","coreVersion":"10.290","createdTime":1669629693662,"modifiedTime":1669629693662,"lastModifiedBy":"tXyJLVgERViy4mQ6"},"folder":null,"sort":0,"ownership":{"default":0,"tXyJLVgERViy4mQ6":3},"_id":"Tu0ZnZ4YK09o5JdP"}
