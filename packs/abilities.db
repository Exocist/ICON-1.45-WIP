/p><p><span id=\"page16R_mcid51\"><br role=\"presentation\"></span><span dir=\"ltr\" role=\"presentation\">Mastery: VORTICE DI FOLLI</span><span id=\"page16R_mcid52\"><br role=\"presentation\"></span><span dir=\"ltr\" role=\"presentation\">Spinning top can be taken twice a turn.</span></p>","quantity":1,"weight":0,"attributes":{"Tags":{"Actions":{"value":"2 Actions","label":"Actions","dtype":"String","group":"Tags"},"Tag1":{"value":"Melee Attack","label":"","dtype":"String","group":"Tags"},"Tag2":{"value":"True Strike","label":"","dtype":"String","group":"Tags"},"Tag3":{"value":"","label":"","dtype":"String","group":"Tags"}}},"groups":{"Tags":{"label":"Tags","dtype":"String"}}},"_stats":{"systemId":"worldbuilding","systemVersion":"0.7.1","coreVersion":"10.288","createdTime":null,"modifiedTime":1665828057080,"lastModifiedBy":"tXyJLVgERViy4mQ6"},"folder":null,"sort":0,"ownership":{"default":0,"tXyJLVgERViy4mQ6":3},"_id":"zkEghjHlLzTueU8M"}
